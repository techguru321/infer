.source_language = "hack"

type HackString = { val: *string }

define $builtins.hack_string(str: *string) : *HackMixed {
  #entry:
    n0 = __sil_allocate(<HackString>)
    n1: *string = load &str
    store n0.HackString.val <- n1 : *string
    ret n0
}
