@SET_MAKE@
ROOT_DIR = .
include $(ROOT_DIR)/Makefile.config

all: clang java

java:
	$(MAKE) -C $(INFER_DIR) java

clang:
	$(MAKE) -C $(INFER_DIR) clang

clean:
	$(MAKE) -C $(INFER_DIR) clean

.PHONY: all clean clang java
